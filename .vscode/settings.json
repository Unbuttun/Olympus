{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.associations": {
    ".env.**": "dotenv"
  },
  "search.exclude": {
    "**/lib/": true,
    "yarn-error.log": true,
    "yarn.lock": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "conventionalCommits.scopes": [
    "yarn2",
    "generated",
    "docs",
    "deps",
    "types",
    "deepsource",
    "gaia-email",
    "athena-urql",
    "gaia",
    "athena",
    "olympus"
  ]
}
